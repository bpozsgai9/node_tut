    <%- include('./partials/header.ejs') %>
    <div class="blogs content">
        <h2>All Blogs</h2>
        <% if (blogs.length > 0) { %>
            <% blogs.forEach((e) => { %>
                <h3 class="title"><%= e.title %></h3>
                <p class="snippet"><%= e.snippet %></p>
            <% }) %>
        <% } else { %>
            <p>No blogs to display</p>
        <% } %>
    </div>

    <%- include('./partials/footer.ejs') %>